class ToggleCase {
    public static void main(String[] args) {
        String s = "JaVa";
        String result = "";

        for (int i = 0; i < s.length(); i++) {
            char c = s.charAt(i);

            if (Character.isUpperCase(c))
                result += Character.toLowerCase(c);
            else
                result += Character.toUpperCase(c);
        }

        System.out.println(result);
    }
}
