class Bank {
    int balance = 1000;

    void deposit(int amount) {
        balance = balance + amount;
        System.out.println("Balance = " + balance);
    }

    void withdraw(int amount) {
        balance = balance - amount;
        System.out.println("Balance = " + balance);
    }

    public static void main(String[] args) {
        Bank b = new Bank();
        b.deposit(500);
        b.withdraw(300);
    }
}
